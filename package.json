{
  "name": "agentforge",
  "version": "0.11.2",
  "private": true,
  "type": "module",
  "description": "Production-ready framework for building deep agents with LangGraph",
  "homepage": "https://tvscoundrel.github.io/agentforge/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TVScoundrel/agentforge.git"
  },
  "bugs": {
    "url": "https://github.com/TVScoundrel/agentforge/issues"
  },
  "keywords": [
    "agents",
    "ai",
    "langchain",
    "langgraph",
    "llm",
    "tools"
  ],
  "author": "Tom Van Schoor",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write \"packages/**/*.ts\"",
    "format:check": "prettier --check \"packages/**/*.ts\"",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "packageManager": "pnpm@10.26.0",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
