  "keywords": [
    "agentforge",
    "ai-agent",
    "customer-support",
    "reusable-agent",
    "langgraph",
    "langchain"
  ],
  "dependencies": {
    "@agentforge/core": "workspace:*",
    "@agentforge/patterns": "workspace:*",
    "@agentforge/tools": "workspace:*",
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}

