{
  "name": "@agentforge/tools",
  "version": "0.10.7",
  "description": "Standard tools collection for AgentForge framework - web, data, file, and utility tools",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentforge/core": "workspace:*",
    "@slack/web-api": "^7.13.0",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^4.5.0",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "@langchain/langgraph": "^1.1.2"
  },
  "peerDependenciesMeta": {
    "@langchain/langgraph": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@langchain/langgraph": "^1.1.2",
    "@types/node": "^22.10.2",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "agentforge",
    "tools",
    "web-tools",
    "data-tools",
    "file-tools",
    "utilities",
    "ai",
    "agents",
    "http",
    "scraper",
    "parser",
    "csv",
    "json",
    "xml"
  ],
  "author": "Tom Van Schoor",
  "license": "MIT",
  "homepage": "https://tvscoundrel.github.io/agentforge/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TVScoundrel/agentforge.git",
    "directory": "packages/tools"
  },
  "bugs": {
    "url": "https://github.com/TVScoundrel/agentforge/issues"
  }
}
