{
  "name": "@agentforge/patterns",
  "version": "0.4.0",
  "description": "Agent patterns (ReAct, Planner-Executor) for AgentForge framework",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "agents",
    "ai",
    "langchain",
    "langgraph",
    "react",
    "patterns"
  ],
  "author": "Tom Van Schoor",
  "license": "MIT",
  "homepage": "https://tvscoundrel.github.io/agentforge/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TVScoundrel/agentforge.git",
    "directory": "packages/patterns"
  },
  "bugs": {
    "url": "https://github.com/TVScoundrel/agentforge/issues"
  },
  "dependencies": {
    "@agentforge/core": "^0.4.0",
    "@langchain/core": "^1.1.0",
    "@langchain/langgraph": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.19.1",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "@langchain/core": "^1.1.0",
    "@langchain/langgraph": "^1.0.0"
  }
}
