{
  "name": "@agentforge/cli",
  "version": "0.1.3",
  "description": "CLI tool for AgentForge - scaffolding, development, and deployment",
  "type": "module",
  "bin": {
    "agentforge": "./bin/agentforge.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "bin",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "agentforge",
    "cli",
    "scaffolding",
    "agent",
    "ai",
    "llm",
    "langchain",
    "langgraph"
  ],
  "author": "Tom Van Schoor",
  "license": "MIT",
  "homepage": "https://tvscoundrel.github.io/agentforge/",
  "bugs": {
    "url": "https://github.com/TVScoundrel/agentforge/issues"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "inquirer": "^12.3.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "zod": "^3.24.1",
    "dotenv": "^16.4.7",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "@eslint/js": "^9.17.0",
    "typescript-eslint": "^8.19.1",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TVScoundrel/agentforge.git",
    "directory": "packages/cli"
  }
}
