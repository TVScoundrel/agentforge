{
  "name": "@agentforge/testing",
  "version": "0.14.0",
  "description": "Testing utilities for TypeScript AI agents, including mock LLMs, mock tools, state builders, fixtures, and workflow assertions.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentforge/core": "workspace:*",
    "@agentforge/patterns": "workspace:*",
    "@langchain/core": "^1.1.17",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "agent-testing",
    "llm-testing",
    "ai-testing",
    "test-utils",
    "mocks",
    "fixtures",
    "workflow-testing",
    "mock-llm",
    "mock-tools",
    "typescript",
    "langchain-typescript"
  ],
  "author": "Tom Van Schoor",
  "license": "MIT",
  "homepage": "https://tvscoundrel.github.io/agentforge/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TVScoundrel/agentforge.git",
    "directory": "packages/testing"
  },
  "bugs": {
    "url": "https://github.com/TVScoundrel/agentforge/issues"
  }
}
